<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.swiftforensics.diskblock</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/disk_block_daemon</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>UserName</key>
    <string>root</string>
    <key>StandardOutPath</key>
    <string>/tmp/diskblock.out.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/diskblock.err.log</string>
</dict>
</plist>
